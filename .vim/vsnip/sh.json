{
    "shebang": {
        "prefix": "shebang",
        "body": [
            "#!/usr/bin/env bash",
            "set -euo pipefail",
            "export LC_ALL=C",
            "cd \"\\$(dirname \"\\$0\")\"",
            "start_time=\\$(date +%s.%N)",
            "",
            "$0",
            "",
            "end_time=\\$(date +%s.%N)",
            "wall_time=\\$(echo \"\\${end_time}\"-\"\\${start_time}\" | bc -l)",
            "cd \"\\$(dirname \"\\$0\")\"",
            "echo [\"\\$(realpath \"\\$0\")\"] WallTime: \"\\${wall_time}\" [s]"
        ]
    }
}
