{
    "shebang-bash": {
        "prefix": "shebang-bash",
        "body": [
            "#!/usr/bin/env bash",
            "set -euox pipefail -o posix",
            "",
            "$0"
        ]
    },
    "shebang-zsh": {
        "prefix": "shebang-zsh",
        "body": [
            "#!/usr/bin/env zsh",
            "set -euo pipefail",
            "setopt xtrace posix err_exit",
            "",
            "$0"
        ]
    }
}
